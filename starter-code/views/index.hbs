<div class="header">
  <h1>{{title}}</h1>
  <div class="buttons">
    <span class="hBut"><a class="btnA findMe" href="#">Find me</a></span>
    <span class="hBut"><a class="btnA" href="/places/add">Add place</a></span>
  </div>
</div>

<script>
  window.places = {{{places}}}
</script>

<div class="container">
  <div id="map"></div>
  <div class="placesList">
    <h2>Places list</h2>
    {{#each placesList}}
      {{> placeList}}
    {{/each}}
  </div>
</div>


<script src="/javascripts/main.js"></script>
